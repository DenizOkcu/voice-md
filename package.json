{
  "name": "obsidian-voice-md",
  "version": "1.2.2",
  "description": "Record audio and transcribe it to markdown using OpenAI Audio Transcription API",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "update-version": "node update-version.mjs"
  },
  "keywords": [
    "obsidian",
    "plugin",
    "voice",
    "transcription",
    "gpt-4o-transcribe",
    "openai"
  ],
  "author": "Deniz Okcu",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:DenizOkcu/voice-md.git"
  },
  "homepage": "https://github.com/DenizOkcu/voice-md#readme",
  "bugs": {
    "url": "https://github.com/DenizOkcu/voice-md/issues"
  },
  "dependencies": {
    "openai": "^6.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@types/node": "^24.8.1",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "builtin-modules": "^5.0.0",
    "esbuild": "^0.25.11",
    "eslint": "^9.38.0",
    "globals": "^16.4.0",
    "obsidian": "latest",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3"
  }
}
